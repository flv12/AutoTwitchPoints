{
	"manifest_version": 2,
	"name": "AutoTwitchPoints",
	"version": "1.1",

	"description": "Automatically collects twitch channel points",

	"icons": {
		"48": "icons/icon-48.png",
		"96": "icons/icon-96.png"
	},

	"content_scripts": [
		{
			"matches": ["*://*.twitch.tv/*"],
			"js": ["js/app.js"]
		}
	]
}
